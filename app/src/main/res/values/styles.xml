<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NotEditable">
        <item name="android:clickable">@string/false_string</item>
        <item name="android:cursorVisible">@string/false_string</item>
        <item name="android:focusable">@string/false_string</item>
        <item name="android:focusableInTouchMode">@string/false_string</item>
    </style>

</resources>
